// Maxcd.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main() 
{
int x,y,z,i,a,b;
z=1;
i=2;

  cout<<"Meta dos numeros: ";
  cin>>x;
  cin>>y;
  if(x<y)
  { 
    a=x;
    b=y;
  }
  else
  { b=x;
    a=y;
  }
  do{
	  if(x%i==0 && y%i==0)
	  { z=z*i;
	    a=a/i;
		b=b/i;
	  }
	  else{i++;}
    }
	  while(i<=a);
	  cout<<"el mcd es: "<<z;
	  getch();
}
