// impuesto.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

int main()
{   float s,sm,i;
    cout<<"Ingrese el salario :";
	cin>> s;
	cout<<"Ingrese el salario Minimo :";
	cin>> sm;
	if (s>=2*sm)
	{
	  i=(s*0.11);
      cout<<"Pagara impuesto: "<<i;
	}  
	else
	  cout<<"no pagara impuesto";
	   
	getch();
	return 0;
}
